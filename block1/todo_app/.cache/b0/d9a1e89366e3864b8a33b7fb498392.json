e;\n\tborder-spacing: 0;\n}\n/*end reset.scss*/","partials/_variables.scss":"\n/* -------------------------------- \n\nColors\n\n-------------------------------- */\n$color-accent: #DFEBF2; // Light Blue\n$color-darkest: #26211F; // Dark Brown\n$color-med: #9BB1BF; // medium blue\n\n\n","partials/_typography.scss":"/* -------------------------------- \n\nFonts\n\n-------------------------------- */\n\n$primary-font: 'Open+Sans', sans-serif;\n$display-font: 'Merriweather', cursive;\n\n\n/* -------------------------------- \n\nxTypography \n\n-------------------------------- */\nbody{\n  font-family: $primary-font;\n}\nh1,\nh2 {\n  font-family: $display-font;\n  color: $color-accent;\n}\n\nh1 {\n  font-size: 3rem;\n  font-weight: 300;\n  line-height: 1.2;\n  padding:1rem;\n\n}\n\nh2 {\n  font-weight: 300;\n  font-size: 2rem;\n  padding:0 1rem;\n  \n}\n\nh3{\n    font-family: $display-font;\n    font-size: 1.5rem;\n    font-weight: 300;\n    color: $color-med;\n    padding:1rem;\n}\n\np {\n  color: $color-med;\n  padding-top:.2rem;\n    \n}\n\na {\n  color: $color-darkest;\n  text-decoration: none;\n  @include transition(0.5s);\n    \n    &:hover{\n        font-size: 1.2em;\n        color: $color-med;\n    }\n}\n\n\n\n\n//end typography.scss","partials/_prefix-mixins.scss":"@mixin box-sizing($type) {\n  -webkit-box-sizing: $type;\n  -moz-box-sizing: $type;\n  box-sizing: $type;\n}\n\n@mixin clearfix {\n  &::after {\n    clear: both;\n    content: \"\";\n    display: block;\n  }\n}\n\n@mixin transform($options) {\n  -webkit-transform: $options;\n  -moz-transform: $options;\n  -ms-transform: $options;\n  -o-transform: $options;\n  transform: $options;\n}\n\n@mixin transition($options) {\n  -webkit-transition: $options;\n  -moz-transition: $options;\n  transition: $options;\n}\n","partials/_basic-style.scss":"/* -------------------------------- \n\nxBasic style\n\n-------------------------------- */\n//basic styles.scss\n*, *::after, *::before {\n\t@include box-sizing(border-box);\n}\n\nhtml * {\n\t-webkit-font-smoothing: antialiased;\n\t-moz-osx-font-smoothing: grayscale;\n}\n\nbody {\n    width: 100%;\n\tmargin: 0 auto;\n\t\n}\nmain {\n\twidth: 100%;\n\n}\n\nheader {\n\twidth: 100%;\n\n}\nli{\n\tpadding:.5rem 1rem;\n}\nfooter{\n\twidth:100%;\n}\n//end basic-styles.scss\n","partials/_header.scss":"\n/* -------------------------------- \n\nHeader\n\n-------------------------------- */\n//header.scss\nheader {\n\n  section{\n    background-color:$color-med;\n    padding:1rem;\n  }\n\n  img{\n    float: left;\n    width:40%;\n    clip-path: circle();\n    -webkit-clip-path:circle();\n    margin:2%;\n    shape-outside: circle();\n    \n  }  \n  .pic-wraper{\n    filter: drop-shadow(-3px 20px 15px rgba(0, 0, 0, 0.5));\n    -webkit-filter: drop-shadow(-3px 20px 15px rgba(0, 0, 0, 0.5));\n  }\n  \n  }\n\n  //end header.scss","partials/_main.scss":"/* -------------------------------- \n\nMain Styling\n\n-------------------------------- */\n\n// Layout for the block sections\n.container{\n  display:flex;\n  flex-flow: row wrap;\n}\n\n","partials/_footer.scss":"/* -------------------------------- \n\nfooter\n\n-------------------------------- */ \nfooter{\n    background-color: $color-med;\n    padding:1rem;\n    margin:1rem 0;   \n}\n.toDo{\n    display:flex;\n    justify-content: space-between;\n}\n#task, #button{\n    margin: 0 .5rem;\n    padding: 1rem;\n    border-radius: 10px;\n    filter: drop-shadow(-3px 20px 15px rgba(0, 0, 0, 0.5));\n    -webkit-filter: drop-shadow(-3px 20px 15px rgba(0, 0, 0, 0.5));\n}\n\nfooter{\n    p{\n      font-size: 1rem;\n      color:$color-darkest;\n      padding:1rem;\n    } \n  }\n"},"lineCount":null}},"error":null,"hash":"3e9a23289e30db7bb348866af4039349","cacheData":{"env":{}}}